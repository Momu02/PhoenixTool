          layout_width="25dp";
          layout_marginLeft="2.5dp";
          layout_marginTop="1dp";
          layout_gravity="center";
          colorFilter="#FFFFCD36";
          src="image/home2.png";
        }
      },
      {
        FrameLayout;
        layout_height="fill";
        layout_width="fill";
        layout_weight="1";
        {
          CardView;
          layout_height="50dp";
          layout_width="50dp";
          layout_gravity="center";
          radius="25dp";
          elevation="0";
          CardBackgroundColor="#ffffffff";
          onClick = function(v)
            旋转动画(ftb, {0, 135}, 300)
            旋转动画(ftb2, {0, 135}, 300)
            view = page_forum -- 视角定位为论坛
            view.destroyDrawingCache() -- 破坏绘画缓存
            view.setDrawingCacheEnabled(true) -- 设置为可缓存绘画
            view.buildDrawingCache() -- 建立绘画缓存
            bmp = view.getDrawingCache() -- 获取绘画缓存，也就是相当于这个aly截个图，这就是图片

            tan_chuang.addView(loadlayout({
              ImageView;
              layout_height = "fill";
              layout_width = "fill";
              scaleType = "fitXY";
              ImageBitmap = qing高斯模糊(activity, 缩略图片(bmp, 50), 1, 2);
              onClick = function(v)
                旋转动画(ftb, {135, 0}, 300)
                旋转动画(ftb2, {135, 0}, 300)

                -- 创建圆形揭露动画
                animator = ViewAnimationUtils.createCircularReveal(tan_chuang, activity.getWidth() / 2, activity.getHeight(), Math.hypot(tan_chuang.getWidth(), tan_chuang.getHeight()), 0)
                animator.setInterpolator(AccelerateInterpolator())
                animator.setDuration(400)
                animator.start()
                -- 添加动画结束监听器
                animator.addListener({
                  onAnimationEnd = function(animation)
                    tan_chuang.setVisibility(View.INVISIBLE) -- 动画结束后隐藏布局
                  end
                })
                -- 执行纵振动画
                纵振动画(ftb_one, "Y", {0, -30, 0, 30, 0}, 400)
                纵振动画(ftb_two, "Y", {0, -30, 0, 30, 0}, 450)
                纵振动画(ftb_three, "Y", {0, -30, 0, 30, 0}, 500)
                纵振动画(ftb_four, "Y", {0, -30, 0, 30, 0}, 550)
                纵振动画(ftb_five, "Y", {0, -30, 0, 30, 0}, 600)
                纵振动画(ftb_six, "Y", {0, -30, 0, 30, 0}, 750)
                纵振动画(ftb_seven, "Y", {0, -30, 0, 30, 0}, 800)
                纵振动画(ftb_eight, "Y", {0, -30, 0, 30, 0}, 850)
              end,
            }), 0)

            -- 设置可见性并启动揭露动画
            tan_chuang.setVisibility(View.VISIBLE)
            animator = ViewAnimationUtils.createCircularReveal(tan_chuang, activity.getWidth() / 2, activity.getHeight(), 0, Math.hypot(tan_chuang.getWidth(), tan_chuang.getHeight()))
            animator.setInterpolator(AccelerateInterpolator())
            animator.setDuration(400)


            animator.start()

            -- 执行纵振动画
            纵振动画(ftb_one, "Y", {0, -30, 0, 30, 0}, 400)
            纵振动画(ftb_two, "Y", {0, -30, 0, 30, 0}, 450)
            纵振动画(ftb_three, "Y", {0, -30, 0, 30, 0}, 500)
            纵振动画(ftb_four, "Y", {0, -30, 0, 30, 0}, 550)
            纵振动画(ftb_five, "Y", {0, -30, 0, 30, 0}, 600)
            纵振动画(ftb_six, "Y", {0, -30, 0, 30, 0}, 750)
            纵振动画(ftb_seven, "Y", {0, -30, 0, 30, 0}, 800)
            纵振动画(ftb_eight, "Y", {0, -30, 0, 30, 0}, 850)
          end;
          {
            ImageView;
            id="ftb";
            layout_height="20dp";
            layout_width="20dp";
            layout_gravity="center";
            colorFilter="#80000000";
            src="image/plus.png";
          }
        }
