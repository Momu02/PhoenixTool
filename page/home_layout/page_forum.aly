    FrameLayout;
    id="bottom_bar";
    layout_height="70dp";
    layout_width="fill";
    elevation="9";
    layout_gravity="bottom";
    {
      CardView;
      layout_height=activity.getHeight();
      layout_width=activity.getHeight();
      layout_gravity="center|top";
      radius=activity.getHeight()/2;
      elevation="10";
      CardBackgroundColor=主题色;
    },
    {
      LinearLayout;
      layout_height="fill";
      layout_width="fill";
      elevation="11";
      orientation="horizontal";
      {
        FrameLayout;
        layout_height="fill";
        layout_width="fill";
        layout_weight="1";
      },
      {
        FrameLayout;
        layout_height="fill";
        layout_width="fill";
        layout_weight="1";
