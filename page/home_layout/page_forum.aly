          pagef.showPage(0)
          水珠动画(v,500)
        end,
        {
          ImageView;
          layout_height="25dp";
          layout_width="25dp";
          layout_gravity="center";
          src="image/home.png";
        },
        {
          ImageView;
          id="bottom_bar_one";
